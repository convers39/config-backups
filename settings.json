{
  "[blade]": {
    "editor.autoClosingBrackets": "always",
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[c]": {
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[sass]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": [
    "django-html",
    "django-txt",
    "dockerfile",
    "sass",
    "sql",
    "toml",
    "vue",
    "vue-html"
  ],
  "cSpell.userWords": [
    "auth",
    "birthdate",
    "camelize",
    "formik",
    "microservices",
    "romaji",
    "Spatie",
    "voyager",
    "zippo"
  ],
  // "editor.codeActionsOnSave": {
  //   "source.fixAll.eslint": true
  // },
  "editor.cursorStyle": "line-thin",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Operator Mono Lig",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.lineNumbers": "relative",
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.tokenColorCustomizations": {
    "[Material Theme]": {
      "semanticHighlighting": true
    }
  },
  "emmet.showSuggestionsAsSnippets": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.extension": "language",
    "*.scss": "sass",
    "*css": "css",
    "*html": "html",
    "*py": "python",
    "*.tsx": "typescriptreact"
  },
  "files.autoSave": "onFocusChange",
  "html-css-class-completion.enableEmmetSupport": true,
  "html.autoClosingTags": true,
  "html.format.endWithNewline": true,
  "html.suggest.angular1": true,
  "html.suggest.html5": true,
  "html.suggest.ionic": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.disableJupyterAutoStart": true,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "jupyter.themeMatplotlibPlots": true,
  "liveServer.settings.donotShowInfoMsg": true,
  // "prettier.disableLanguages": ["javascript", "javascriptreact"],
  "pasteImage.namePrefix": "${currentFileNameWithoutExt}_",
  "pasteImage.path": "${projectRoot}/assets",
  "pasteImage.basePath": "${projectRoot}/",
  "pasteImage.forceUnixStyleSeparator": true,
  "pasteImage.prefix": "/",
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "python.pythonPath": "/usr/local/bin/python3",
  "python.formatting.provider": "autopep8",
  "python.jediEnabled": false,
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "pythonTestExplorer.testFramework": "pytest",
  "sync.autoUpload": true,
  "sync.gist": "f0e45df9d5418322b4671e2b0690fdd8",
  "sync.quietSync": true,
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.fontFamily": "FiraCode-Retina",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "todohighlight.defaultStyle": {
    "backgroundColor": "#ffab00",
    "border": "1px solid #eee",
    "borderRadius": "2px",
    "color": "red",
    "cursor": "pointer",
    "isWholeLine": false,
    "overviewRulerColor": "#ffab00"
  },
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.go",
    "**/*.py"
  ],
  "todohighlight.isCaseSensitive": true,
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    {
      "backgroundColor": "yellow",
      "color": "#ff0000",
      "overviewRulerColor": "grey",
      "text": "NOTE:"
    },
    {
      "color": "#000",
      "isWholeLine": false,
      "text": "HACK:"
    },
    {
      "backgroundColor": "rgba(0,0,0,.2)",
      "border": "1px solid red",
      "borderRadius": "2px",
      "color": "red",
      "text": "TODO:"
    }
  ],
  "todohighlight.keywordsPattern": "TODO:|FIXME:",
  "todohighlight.maxFilesForSearch": 5120,
  "todohighlight.toggleURI": false,
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "semi": false,
      "singleQuote": true,
      "wrap_attributes": "force-aligned",
      "wrap_line_length": 200,
      "wrap_width_line": false
    },
    "prettier": {
      "Semi": false,
      "Singlequote": true
    },
    "prettyhtml": {
      "printWidth": 100,
      "singleQuote": false,
      "sortAttributes": true,
      "wrapAttributes": false
    }
  },
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>", "l"],
      "before": ["j", "j"]
    },
    {
      "after": ["<Esc>", "l"],
      "before": ["j", "k"]
    }
  ],
  "vim.visualModeKeyBindings": [
    // {
    //   "before": ["J"],
    //   "after": ["m", "'", ">", "+", "1"]
    // },
    // {
    //   "before": ["K"],
    //   "after": ["m", "'", "<", "-", "2"]
    // }
  ],
  // vnoremap J :m '>+1<CR>gv=gv
  // vnoremap K :m '<-2<CR>gv=gv
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":noh"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "w", "f"],
      "commands": ["workbench.action.toggleEditorWidths"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // {
    //   "before": ["<leader>", "c"],
    //   "commands": ["workbench.action."]
    // },
    {
      "after": ["d", "d"],
      "before": ["<leader>", "d"]
    },
    {
      "before": ["K"],
      "commands": [{ "command": "editor.action.showHover" }]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<leader>", "u"],
      "after": ["<C-u>"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["<C-d>"]
    },
    {
      "before": ["<leader>", "w", "j"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<leader>", "w", "k"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<leader>", "w", "h"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<leader>", "w", "l"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["<leader>", "w", "v"],
      "after": ["<C-w>", "v"]
    },
    {
      "before": ["<leader>", "w", "s"],
      "after": ["<C-w>", "s"]
    },
    {
      "before": ["<leader>", "t", "n"],
      "commands": [":tabn"]
    },
    {
      "before": ["<leader>", "t", "p"],
      "commands": [":tabp"]
    }
  ],
  "vim.relativenumber": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.sideBar.location": "left",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.startupEditor": "newUntitledFile",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "bladeFormatter.format.indentSize": 4,
  "C_Cpp.updateChannel": "Insiders",
  "code-runner.runInTerminal": true,
  "debug.console.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "workbench.iconTheme": "file-icons-colourless",
  // "python.pythonPath": "/home/linuxbrew/.linuxbrew/bin/python3",
  "workbench.colorTheme": "Bearded Theme Oceanic",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "php": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "liveshare.sharedTerminalWidth": 180,
  "editor.formatOnSave": true,
  "workbench.editor.untitled.hint": "hidden",
  "vsicons.dontShowNewVersionMessage": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.extension.print.absoluteImgPath": false,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "window.zoomLevel": 1
  // "javascript.validate.enable": false,
  // "javascript.format.enable": false,
  // "standard.enable": true,
  // "standard.autoFixOnSave": true,
  // "standard.run": "onSave",
  // "standard.validate": [
  //   "javascript",
  //   "javascriptreact",
  //   {
  //     "language": "html",
  //     "autoFix": true
  //   }
  // ]
}
